<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>查询到站</title>
    <script src="~/Assets/js/treeview/bootstrap-treeview.js"></script>
    <script src="~/Assets/js/treeview/jquery.js"></script>
    <link href="~/Assets/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <div id="tree"></div>
    <script type="text/javascript">
        $(function () {
            $('#tree').treeview({ data: getTree() });
        });


        function getTree() {
            var data = [
                {
                    text: "Parent 1",
                    nodes: [
                        {
                            text: "Child 1",
                            nodes: [
                                {
                                    text: "Grandchild 1"
                                },
                                {
                                    text: "Grandchild 2"
                                }
                            ]
                        },
                        {
                            text: "Child 2"
                        }
                    ]
                },
                {
                    text: "Parent 2"
                },
                {
                    text: "Parent 3"
                },
                {
                    text: "Parent 4"
                },
                {
                    text: "Parent 5"
                }
            ];
            return data;
        }
    </script>
</body>
</html>
