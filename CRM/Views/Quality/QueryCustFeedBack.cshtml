@model NF.Framework.DTO.TMQ_QUALITY_MAINDTO
@using NF.Web.Core
<!DOCTYPE html>

<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <script src="~/Assets/js/My97DatePicker/WdatePicker.js"></script>
    @Html.Partial("~/Views/Shared/_Assets.cshtml")
</head>
<body>
    <table class="table table-bordered table-condensed">
        <tr>
            <td style="width:100px;" class="strong">
                客户
            </td>
            <td style=" width:400px;">
                @Model.C_CUST_NAME
            </td>
            <td  style="width:100px;" class="strong" >
                客户类型
            </td>
            <td colspan="3" >
                @Html.DetailName(Model.C_CUST_TYPE == null ? "1" : Model.C_CUST_TYPE, "CustType")
            </td>
        </tr>
        <tr>
            <td class="strong">
                联系人
            </td>
            <td>
                @Model.C_NAME

            </td>

            <td class="strong">
                联系电话
            </td>
            <td colspan="3" >
                @Model.C_TEL

            </td>
        </tr>
        <tr>
            <td class="strong">
                原始盘条样
            </td>
            <td>
                @Model.N_PARENT_QUA

            </td>
            <td class="strong">
                中间产品样
            </td>
            <td>
                @Model.N_MIDDLE_QUA

            </td>
            <td style="width:100px;" class="strong">
                问题产品样
            </td>
            <td>
                @Model.N_QUEST_QUA

            </td>
        </tr>
        <tr>
            <td class="strong">
                生成工艺
            </td>
            <td colspan="6">
                @Model.C_TECH_DESC

            </td>
        </tr>
        <tr>
            <td class="strong">
                信息内容
            </td>
            <td colspan="6">
                @Model.C_SITE_SURVEY_CONTENT
            </td>
        </tr>
    </table>

    <table class="table table-bordered table-hover table-condensed">
        <thead>
            <tr>
                @if (Model.N_FLAG == 1)
                {
                    <th width="120px">牌号</th>
                    <th width="150px">规格</th>
                    <th width="150px">数量</th>
                    <th width="120px">批号</th>
                }
                else
                {
                    <th width="120px">牌号</th>
                    <th width="150px">规格</th>
                    <th width="120px">批号</th>
                    <th width="120px">发货数量</th>
                    <th width="120px">异议数量</th>
                    <th width="120px">执行标准</th>
                }
            </tr>
        </thead>
        <tbody>
            @{
                if (Model != null && Model.QualityStlGrds != null && Model.QualityStlGrds.Count > 0)
                {
                    for (int i = 0; i < Model.QualityStlGrds.Count; i++)
                    {
                        <tr>
                            @if (Model.N_FLAG == 1)
                            {
                                <td>@Model.QualityStlGrds[i].C_STL_GRD</td>
                                <td>@Model.QualityStlGrds[i].C_SPEC mm</td>
                                <td>@Model.QualityStlGrds[i].N_WGT  吨</td>
                                            <td>@Model.QualityStlGrds[i].C_BATCH_NO</td>
                            }
                            else
                            {
                                <td>@Model.QualityStlGrds[i].C_STL_GRD</td>
                                <td>@Model.QualityStlGrds[i].C_SPEC mm</td>
                                <td>@Model.QualityStlGrds[i].C_BATCH_NO</td>
                                            <td>@Model.QualityStlGrds[i].N_SHIP_WGT 吨</td>
                                            <td>@Model.QualityStlGrds[i].N_OBJEC_WGT 吨</td>
                                            <td>@Model.QualityStlGrds[i].C_STD_CODE</td>
                            }
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</body>
</html>
