@model NF.Framework.DTO.TMQ_QUALITY_MAINDTO
@using NF.Web.Core
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@Model.Title</title>
    <script src="~/Assets/js/My97DatePicker/WdatePicker.js"></script>
    @Html.Partial("~/Views/Shared/_Assets.cshtml")
</head>

<body>

    @using (Html.BeginForm())
    {
        <table class="table table-bordered table-condensed">
            <tr>
                <td style="width:100px;" class="strong">
                    地区
                </td>
                <td colspan="3">
                    @Model.C_AREA_NAME
                </td>
            </tr>
            <tr>
                <td class="strong">
                    客户
                </td>
                <td>
                    @Model.C_CUST_NAME
                </td>

                <td style="width:100px;" class="strong">
                    客户类型
                </td>
                <td>
                    @Html.DetailName(Model.C_CUST_TYPE == null ? "1" : Model.C_CUST_TYPE, "CustType")
                </td>
            </tr>
            <tr>
                <td class="strong">
                    联系人
                </td>
                <td>
                    @Model.C_NAME
                </td>

                <td class="strong">
                    联系电话
                </td>
                <td>
                    @Model.C_TEL
                </td>
            </tr>
            <tr>
                <td class="strong">
                    产品
                </td>
                <td>
                    @Model.C_STL_GRD_CLASS
                </td>
                <td class="strong">
                    产品用途
                </td>
                <td>
                    @Model.C_PROD_USE
                </td>
            </tr>
            <tr>
                <td class="strong">
                    发货时间
                </td>
                <td>
                    @(Model.D_SHIP_START_DT == null ? "" : DateTime.Parse(Model.D_SHIP_START_DT.ToString()).ToString("yyyy-MM-dd"))
                </td>
                <td class="strong">
                    到货时间
                </td>
                <td>
                    @(Model.D_SHIP_END_DT == null ? "" : DateTime.Parse(Model.D_SHIP_END_DT.ToString()).ToString("yyyy-MM-dd"))
                </td>
            </tr>

            <tr>
                <td class="strong">
                    投诉异议内容
                </td>
                <td colspan="3">
                    @Model.C_OBJECT_CONTENT
                </td>
            </tr>
            <tr>
                <td class="strong">
                    用户工艺流程
                </td>
                <td colspan="3">
                    @Model.C_TECH_DESC
                </td>
            </tr>
            <tr>
                <td class="strong">
                    现场调查情况
                </td>
                <td colspan="3">
                    @Model.C_SITE_SURVEY_CONTENT
                </td>
            </tr>
            <tr>
                <td class="strong">
                    母材
                </td>
                <td>
                    @Model.N_PARENT_QUA
                </td>
                <td class="strong">
                    问题样支数
                </td>
                <td>
                    @Model.N_QUEST_QUA
                </td>
            </tr>
            <tr>
                <td class="strong">
                    中间样支数
                </td>
                <td>
                    @Model.N_MIDDLE_QUA
                </td>
                <td class="strong">
                    其他支数
                </td>
                <td>
                    @Model.N_ELSE_QUA
                </td>
            </tr>
            <tr>
                <td class="strong">
                    本部门
                </td>
                <td>
                    @Model.C_DEPT
                </td>
                <td class="strong">
                    质控部
                </td>
                <td>
                    @Model.C_DEPT2
                </td>
            </tr>
            <tr>
                <td class="strong">
                    技术中心
                </td>
                <td>
                    @Model.C_DEPT3
                </td>
                <td class="strong">
                    其他部门
                </td>
                <td>
                    @Model.C_DEPT4
                </td>
            </tr>
            <tr>
                <td class="strong">
                    备注
                </td>
                <td colspan="3">
                    @Model.C_REMARK
                </td>
            </tr>
        </table>
        <table class="table table-bordered table-condensed">
            <tr>
                <td class="strong" style="width:100px;">
                    修改人
                </td>
                <td>
                    @Model.C_EMP_NAME
                </td>
                <td class="strong" style="width:100px;">
                    修改时间
                </td>
                <td>
                    @Model.D_MOD_DT
                </td>
                <td class="strong" style="width:100px;">
                    审批人
                </td>
                <td>
                    @Model.C_APPROVER
                </td>
                <td class="strong" style="width:100px;">
                    审批时间
                </td>
                <td>
                    @Model.D_APPROVER_DT
                </td>
            </tr>
        </table>
    }
    <table class="table table-bordered table-hover table-condensed">
        <thead>
            <tr>
                @if (Model.N_FLAG == 1)
                {
                    <th>牌号</th>
                    <th>规格</th>
                    <th>数量</th>
                    <th>批号</th>
                }
                else
                {
                    <th>牌号</th>
                    <th>规格</th>
                    <th>批号</th>
                    <th>发货数量</th>
                    <th>异议数量</th>
                    <th>执行标准</th>
                }

            </tr>
        </thead>
        <tbody>
            @{
                if (Model != null && Model.QualityStlGrds != null && Model.QualityStlGrds.Count > 0)
                {
                    for (int i = 0; i < Model.QualityStlGrds.Count; i++)
                    {
                        <tr>
                            @if (Model.N_FLAG == 1)
                            {
                                <td>@Model.QualityStlGrds[i].C_STL_GRD</td>
                                <td>@Model.QualityStlGrds[i].C_SPEC mm</td>
                                <td>@Model.QualityStlGrds[i].N_WGT  吨</td>
                                <td>@Model.QualityStlGrds[i].C_BATCH_NO</td>
                            }
                            else
                            {
                                <td>@Model.QualityStlGrds[i].C_STL_GRD</td>
                                <td>@Model.QualityStlGrds[i].C_SPEC mm</td>
                                <td>@Model.QualityStlGrds[i].C_BATCH_NO</td>
                                <td>@Model.QualityStlGrds[i].N_SHIP_WGT 吨</td>
                                <td>@Model.QualityStlGrds[i].N_OBJEC_WGT 吨</td>
                                <td>@Model.QualityStlGrds[i].C_STD_CODE</td>
                            }
                        </tr>
                    }
                }
            }
        </tbody>
    </table>

</body>
</html>
